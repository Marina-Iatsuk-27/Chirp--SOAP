<xs:annotation>
<xs:documentation>Y axis position.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="zPosition" minOccurs="0" maxOccurs="1" type="xs:string" sawsdl:modelReference="http://iec.ch/TC57/2016/CIM-schema-cim17#PositionPoint.zPosition">
<xs:annotation>
