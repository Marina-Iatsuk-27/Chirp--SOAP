</xs:annotation>
</xs:element>
<xs:element name="estimatedLoad" minOccurs="0" maxOccurs="1" type="m:CurrentFlow" sawsdl:modelReference="http://iec.ch/TC57/2016/CIM-schema-cim17#UsagePoint.estimatedLoad">
