<xs:element name="effectiveDateTime" minOccurs="0" maxOccurs="1" type="xs:dateTime" sawsdl:modelReference="http://iec.ch/TC57/2016/CIM-schema-cim17#ConfigurationEvent.effectiveDateTime">
