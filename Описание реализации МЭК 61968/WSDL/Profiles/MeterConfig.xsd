<xs:annotation>
<xs:documentation>If true, this is a solid state end device (as opposed to a mechanical or electromechanical device).</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="phaseCount" minOccurs="0" maxOccurs="1" type="xs:integer" sawsdl:modelReference="http://iec.ch/TC57/2016/CIM-schema-cim17#EndDeviceInfo.phaseCount">
